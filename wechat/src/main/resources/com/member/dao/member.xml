<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xunjiu.member.dao.MemberDao">
    <resultMap id="BaseResultMap" type="com.xunjiu.member.model.Member">
        <id column="id" jdbcType="BIGINT" property="id" />
        <id column="nick_name" jdbcType="VARCHAR" property="nickName" />
    </resultMap>

    <insert id="insert" parameterType="com.xunjiu.member.model.Member" useGeneratedKeys="true" keyProperty="id">
        insert into member(nick_name)
        values (#{nickName})
    </insert>

    <select id="findMember" parameterType="int" resultMap="BaseResultMap">
        select id, nick_name
        from member
        where id = #{id}
    </select>
</mapper>